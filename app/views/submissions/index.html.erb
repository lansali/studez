<p class="notice"><%= notice %></p>
<% @submissions.each do |submission| %>
  <div class="">
    <svg class="" width="100%" height="180" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img" aria-label="Placeholder: Image cap"><title>Placeholder</title><rect width="100%" height="100%" fill="#868e96"></rect><text x="50%" y="50%" fill="#dee2e6" dy=".3em">Image cap</text></svg>
    <div class="">
      <h5 class=""><%= submission.opportunity_id %></h5>
      <p class=""><%= submission.student_id %></p>
      <p class=""><small class=""><%= submission.updated_at %></small></p>
      <%= link_to 'Show', submission, class: "" %>
      <% if current_user.id == submission.student_id %>
        <%= link_to 'Edit', edit_submission_path(submission), class: "" %>
        <%= link_to 'Destroy', submission, method: :delete, data: { confirm: 'Are you sure?' }, class: "" %>
      <% end %>
    </div>
  </div>
  <br/>
<% end %>
